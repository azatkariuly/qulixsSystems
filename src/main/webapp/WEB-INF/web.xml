<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
    <welcome-file-list>
		<welcome-file>projectServlet</welcome-file>
	</welcome-file-list>
  <display-name>Archetype Created Web Application</display-name>
  
	<servlet>
	  	<servlet-name>EmployeeServlet</servlet-name>
	  	<servlet-class>com.azatkariuly.controller.EmployeeController</servlet-class>
	</servlet>
	<servlet>
	  	<servlet-name>ProblemServlet</servlet-name>
	  	<servlet-class>com.azatkariuly.controller.ProblemController</servlet-class>
	</servlet>
	<servlet>
	  	<servlet-name>ProjectServlet</servlet-name>
	  	<servlet-class>com.azatkariuly.controller.ProjectController</servlet-class>
	</servlet>
	<servlet-mapping>
	  	<servlet-name>EmployeeServlet</servlet-name>
	  	<url-pattern>/employeeServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	  	<servlet-name>ProblemServlet</servlet-name>
		<url-pattern>/problemServlet</url-pattern>
  	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProjectServlet</servlet-name>
	  	<url-pattern>/projectServlet</url-pattern>
	</servlet-mapping>
	
  	<servlet>
	  	<servlet-name>InitializeDbSerlvet</servlet-name>
	  	<servlet-class>projects.InitializeDbSerlvet</servlet-class>
	</servlet>
	<servlet-mapping>
	  	<servlet-name>InitializeDbSerlvet</servlet-name>
	  	<url-pattern>/InitializeDbSerlvet</url-pattern>
	</servlet-mapping>
</web-app>
